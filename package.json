{
  "name": "number-picture",
  "version": "1.0.0-0",
  "description": "React input component that defers onChange handler until input is blurred",
  "repository": {
    "type": "git",
    "url": "https://github.com/finnfiddle/number-picture"
  },
  "author": "Finn Fitzsimons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finnfiddle/number-picture/issues"
  },
  "homepage": "https://github.com/finnfiddle/number-picture",
  "keywords": [
    "react-component",
    "react"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "start": "babel --watch --plugins \"transform-runtime\" src --ignore __tests__ --out-dir ./dist",
    "prepublish": "npm run lint && npm run build",
    "build": "npm run build:cjs && npm run build:umd && npm run uglify",
    "build:cjs": "babel --plugins 'transform-es2015-modules-umd' src --ignore __tests__ --out-dir ./cjs",
    "build:umd": "rollup -c",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "uglify": "uglifyjs umd/number-picture.js -c -m --source-map umd/number-picture.min.js.map -o umd/number-picture.min.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "gulp-eslint": "^2.1.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-visualizer": "^0.3.1",
    "sinon": "^1.17.3",
    "uglifyify": "^3.0.4"
  },
  "peerDependencies": {
    "react": "~0.14.8 || ^15.0.0",
    "react-dom": "~0.14.8 || ^15.0.0"
  },
  "dependencies": {
    "array-uniq": "^1.0.3",
    "babel-runtime": "^6.20.0",
    "d3-axis": "^1.0.4",
    "d3-ease": "^1.0.3",
    "d3-force": "^1.0.6",
    "d3-hierarchy": "^1.1.5",
    "d3-selection": "^1.0.3",
    "d3-shape": "^1.0.4",
    "d3-transition": "^1.0.3",
    "d3-v4-grid": "^2.0.1",
    "deep-equal": "^1.0.1",
    "intersect": "^1.0.1",
    "its-set": "^1.2.1",
    "react-transition-group": "^1.2.0",
    "utils-deep-get": "^1.0.0"
  }
}
